    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "^0.25.7",
    "esbuild-plugin-tailwindcss": "^2.0.1",
    "nodemon": "^3.1.10",
    "obsidian": "latest",
    "tailwindcss": "^4.1.11",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
