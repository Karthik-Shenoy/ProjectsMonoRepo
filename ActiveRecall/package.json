{
  "name": "active-recall-plugin",
  "version": "1.0.0",
  "description": "An Obsidian plugin for active recall and spaced repetition",
  "main": "main.js",
  "scripts": {
    "watch": "nodemon --watch src --ext ts,tsx,js,jsx --exec \"npm run build\"",
    "build": "tsc -p . -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
